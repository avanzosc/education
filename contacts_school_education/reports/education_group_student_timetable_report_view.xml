<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="education_group_student_timetable_report_view_search" model="ir.ui.view">
        <field name="model">education.group.student.timetable.report</field>
        <field name="inherit_id" ref="education.education_group_student_timetable_report_view_search" />
        <field name="arch" type="xml">
            <field name="center_id" position="attributes">
                <attribute name="domain">[('educational_category','=','school')]</attribute>
            </field>
            <field name="student_id" position="attributes">
                <attribute name="domain">[('educational_category','=','student')]</attribute>
            </field>
        </field>
    </record>
</odoo>
